    "@types/node": "^18.11.10",
    "esbuild": "^0.15.16",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "rollup": "^3.5.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  }
